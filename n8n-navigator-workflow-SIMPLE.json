{
  "name": "Navigator Screenshot Handler (Simple)",
  "nodes": [
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "navigator-screenshot",
        "responseMode": "responseNode",
        "options": {}
      },
      "id": "webhook-node",
      "name": "Screenshot Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [250, 300],
      "webhookId": "navigator-screenshot"
    },
    {
      "parameters": {
        "respondWith": "json",
        "responseBody": "={{ {\n  \"success\": true,\n  \"ui_state\": {\n    \"interpretation\": {\n      \"page_classification\": {\n        \"page_type\": \"dashboard\",\n        \"product_area\": $json.body.tool_detected || \"unknown\",\n        \"confidence\": 0.85\n      },\n      \"ui_elements\": [\n        {\n          \"element_id\": \"btn-main\",\n          \"type\": \"button\",\n          \"label\": \"Primary Action\",\n          \"bounding_box\": {\"x\": 100, \"y\": 100, \"width\": 200, \"height\": 40}\n        }\n      ]\n    }\n  },\n  \"query\": $json.body.query || \"\",\n  \"tool_detected\": $json.body.tool_detected || \"unknown\",\n  \"timestamp\": $now\n} }}",
        "options": {}
      },
      "id": "respond-node",
      "name": "Respond to Webhook",
      "type": "n8n-nodes-base.respondToWebhook",
      "typeVersion": 1,
      "position": [450, 300]
    }
  ],
  "connections": {
    "Screenshot Webhook": {
      "main": [
        [
          {
            "node": "Respond to Webhook",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "active": true,
  "settings": {},
  "versionId": "1",
  "id": "navigator-simple",
  "meta": {
    "instanceId": "local"
  },
  "tags": []
}
